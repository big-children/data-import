{
    "AllowedHosts": "*",
    "AppSettings": {
      "DatabaseEngine": "PostgreSql",
      "AllowTestCertificates": false,
      "EncryptionKey": "$ENCRYPTION_KEY",
      "FileMode": "Local",
      "GoogleAnalyticsMeasurementId": "XXXXXXXXXX",
      "LoginTimeoutInMinutes": 30,
      "ShareName": "C:\\Temp",
      "TemplateIdentityApiEndpoint": "identity",
      "TemplateSharingApiEndpoint": "template-sharing",
      "TemplateSharingApiUrl": "https://template-sharing.ed-fi.org",
      "UsePowerShellWithNoRestrictions": false
    },
    "ConnectionStrings": {
      "defaultConnection": "host=/cloudsql/${PROJECT_ID}:us-central1:edfi-ods-db;port=5432;username=postgres;password=${POSTGRES_PASSWORD};database=EdFi_DataImport;"
    },
    "ExternalPreprocessors": {
      "Enabled": false,
      "TimeOutInMilliseconds": 5000
    }
  }
  